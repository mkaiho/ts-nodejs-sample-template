{
  "name": "ts-nodejs-sample-template",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "yarn serve",
    "serve": "node dist/server.js",
    "build": "yarn build-ts && yarn lint && yarn copy-static-assets",
    "build-ts": "yarn tsc",
    "lint": "eslint --ext .ts,.tsx ./src",
    "copy-static-assets": "yarn ts-node scripts/copyStaticAssets.ts"
  },
  "dependencies": {
    "cookie-parser": "~1.4.5",
    "debug": "~4.1.1",
    "express": "~4.17.1",
    "morgan": "~1.10.0",
    "shelljs": "~0.8.3"
  },
  "devDependencies": {
    "@types/cookie-parser": "~1.4.2",
    "@types/debug": "~4.1.5",
    "@types/express": "~4.17.6",
    "@types/morgan": "~1.9.0",
    "@types/node": "~13.11.1",
    "@types/shelljs": "~0.8.7",
    "@typescript-eslint/eslint-plugin": "~2.28.0",
    "@typescript-eslint/parser": "~2.28.0",
    "eslint": "~6.8.0",
    "eslint-config-airbnb-base": "~14.1.0",
    "eslint-config-prettier": "~6.10.1",
    "eslint-plugin-import": "~2.20.2",
    "eslint-plugin-prettier": "~3.1.3",
    "prettier": "~2.0.4",
    "ts-node": "~8.8.2",
    "typescript": "~3.8.3"
  }
}
